{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": [2, "global"],
        "no-console": [0]

    },
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended"
}